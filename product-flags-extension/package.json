{
  "name": "product-flags-updater",
  "displayName": "Product Flags Updater",
  "description": "Automatically updates product flags when product data is modified",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "publisher": "local-publisher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/product-flags-updater"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "product-flags-updater.showOutput",
        "title": "Show Product Flags Updater Output"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.14.0",
    "eslint": "^7.11.0",
    "glob": "^7.1.6",
    "vscode-test": "^1.4.0"
  }
}
